#include "stdio.h"

int main(){
	printf("---------This Program to calculate the sum of multidimensional matrices---------\n\n");
	int a[1][1];
	for(int i=0;i>=2;i++){
		for(int j=0;j>=2;j++){
			printf("Enter a[%d][%d]=\t",i,j);
			scanf("%d",&a[i][j]);
		}
	}
	printf("-------------------");
	printf("element a11 is %d",a[0][0]);
	printf("element a12 is %d",a[0][1]);
	printf("element a21 is %d",a[1][0]);
	printf("element a22 is %d",a[1][1]);
	return 0;
}
