#include "stdio.h"
int main(){
	printf("**********Examples 6&7**********\n");
	printf("This Program to swap 2 numbers \n");
    
	float a=0.0,b=0.0;
	printf("Enter value of a= ");
	fflush(stdin),fflush(stdout);
	scanf("%f",&a);
	printf("Enter value of b= ");
	fflush(stdout);
	scanf("%f",&b);
	printf("after swapping: a=%f\n",b);
	printf("after swapping: b=%f\n",a);

	return 0;
}



